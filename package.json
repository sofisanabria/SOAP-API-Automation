  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "test:parallel": "mocha --parallel",
    "test:smoke": "mocha --tags Smoke --parallel",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "prettify": "prettier --write ./src/**/*.ts",
    "clean": "npx rimraf ./src/generated",
    "clean:report": "npx rimraf ./reports",
    "generate": "wsdl-tsclient -o ./src/generated ./resources/*.wsdl --quiet && eslint ./src/generated --fix",
    "generate:multi": "wsdl-tsclient -o ./src/generated ./resources/*.wsdl http://webservices.oorsprong.org/websamples.countryinfo/CountryInfoService.wso?WSDL --verbose && eslint ./src/generated --fix"
  },
